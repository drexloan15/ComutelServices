DATABASE_URL="postgresql://postgres:postgres@localhost:5432/comutel_itsm?schema=public"
NODE_ENV="development"
PORT=3001
# BACKEND_PORT es opcional para el orquestador de desarrollo de la raiz.
BACKEND_PORT=3001
CORS_ORIGINS="http://localhost:3000"
CORS_CREDENTIALS="false"
TRUST_PROXY="false"
JWT_ACCESS_SECRET="replace_with_a_long_random_secret_min_32_chars"
JWT_REFRESH_SECRET="replace_with_a_long_random_secret_min_32_chars"
JWT_ACCESS_TTL="15m"
JWT_REFRESH_TTL="7d"
BOOTSTRAP_ADMIN_SECRET="replace_with_a_long_random_secret_min_32_chars"
AUTH_REFRESH_COOKIE_ENABLED="false"
AUTH_REFRESH_COOKIE_NAME="comutel_refresh_token"
AUTH_REFRESH_COOKIE_SECURE="false"
AUTH_REFRESH_COOKIE_SAME_SITE="lax"
AUTH_REFRESH_COOKIE_PATH="/api/auth"
RATE_LIMIT_WINDOW_MS="60000"
RATE_LIMIT_MAX="120"
SLA_ENGINE_AUTORUN_ENABLED="true"
SLA_ENGINE_INTERVAL_MS="60000"
LOG_LEVEL="info"
SEED_DEFAULT_PASSWORD="Password123!"
SEED_ADMIN_EMAIL="admin@comutel.local"
SEED_AGENT_EMAIL="agent@comutel.local"
SEED_REQUESTER_EMAIL="requester@comutel.local"
